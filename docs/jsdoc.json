{
  "source": {
    "include": [
        "./top.md",
        "./react-doc.js"
    ]
  },
  "opts": {
    "access": ["public"],
    "tutorials": "./tutorials",
    "template": "../clean-jsdoc-theme",
    "encoding": "utf8",
    "recurse": false,
    "verbose": true,
    "destination": "../build/croquet-react/",
    "theme_opts": {
        "title": "Croquet React Bindings @CROQUET_VERSION@ References",
        "theme": "light",
        "static_dir": ["../static"],
        "include_css": ["../clean-jsdoc-theme/static/styles/index.css"]
    }
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "includePattern": "\\.(png|jpg)$"
      }
    }
  }
}
