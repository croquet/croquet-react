{
  "name": "croquet-doc-react",
  "version": "1.0.1",
  "description": "Croquet React Bindings",
  "scripts": {
    "start": "npm-watch",
    "build": "npx jsdoc -c jsdoc.json -d build",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "./top.md",
        "./react-doc.js",
        "./tutorials/*",
        "./jsdoc.json"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED"
}
